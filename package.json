{
	"name": "x4codecomplete",
	"displayName": "X4CodeComplete",
	"description": "",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url":"https://github.com/Cgettys/X4CodeComplete"
	},
	"publisher":"CharlieGettys",
	"license": "GPL-3.0",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:xml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "Hello World"
		}]
